{
  "mcp.servers": {
    "azure-logs": {
      "command": "node",
      "args": ["server.js"],
      "cwd": "${workspaceFolder}",
      "env": {
        "AZURE_CLIENT_ID": "${env:AZURE_CLIENT_ID}",
